{
  "agent_name": "Event Selector Agent",
  "version": "1.0.0",
  "description": "ADK agent for searching and selecting events using Exa API",
  "search_params": {
    "num_results": 15,
    "use_autoprompt": true,
    "include_text": true
  },
  "event_types": [
    "concert",
    "theater",
    "comedy",
    "sports",
    "conference",
    "festival",
    "art exhibition",
    "dance performance",
    "opera",
    "musical",
    "film screening",
    "lecture",
    "workshop",
    "networking",
    "food festival"
  ],
  "default_location": "New York City",
  "distance_radius": 25,
  "price_range": {
    "min": 1,
    "max": 4,
    "scale": "1-4 dollar signs"
  },
  "rating_threshold": 4.0,
  "interests": [
    "music",
    "theater",
    "comedy",
    "sports",
    "technology",
    "art",
    "food",
    "dance",
    "film",
    "literature",
    "science",
    "business",
    "networking",
    "wellness"
  ],
  "preferred_domains": [
    "eventbrite.com",
    "ticketmaster.com",
    "stubhub.com",
    "facebook.com",
    "meetup.com",
    "timeout.com",
    "songkick.com",
    "bandsintown.com"
  ],
  "filters": {
    "exclude_keywords": ["cancelled", "postponed", "sold out", "past event"],
    "include_keywords": ["upcoming", "tickets available", "highly rated", "recommended"]
  },
  "output_format": {
    "max_events": 10,
    "include_description": true,
    "include_pricing": true,
    "include_location": true,
    "include_time": true,
    "include_reviews": true
  }
} 