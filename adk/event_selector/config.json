{
  "agent_name": "Event Selector Agent",
  "version": "1.0.0",
  "description": "ADK agent for searching and selecting events using Exa API",
  "search_params": {
    "num_results": 15,
    "start_published_date": "2024-01-01T00:00:00Z",
    "end_published_date": "2024-12-31T23:59:59Z",
    "use_autoprompt": true
  },
  "event_types": [
    "conference",
    "workshop",
    "meetup",
    "seminar",
    "festival",
    "concert",
    "exhibition",
    "networking",
    "hackathon",
    "webinar"
  ],
  "default_location": "San Francisco",
  "location_radius": 50,
  "price_range": {
    "min": 0,
    "max": 500,
    "currency": "USD"
  },
  "rating_threshold": 4.0,
  "preferred_domains": [
    "eventbrite.com",
    "meetup.com",
    "facebook.com",
    "ticketmaster.com",
    "eventful.com",
    "allevents.in"
  ],
  "filters": {
    "exclude_keywords": ["canceled", "postponed", "sold out"],
    "include_keywords": ["open", "available", "registration"]
  },
  "output_format": {
    "max_events": 10,
    "include_description": true,
    "include_pricing": true,
    "include_location": true,
    "include_date": true
  }
} 